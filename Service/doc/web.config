<?xml version="1.0"?>

<configuration>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
    <handlers>
      <add
        name="dCFormRedirector_Integrated"
        preCondition="integratedMode"
        allowPathInfo="true"
        path="*"
        verb="*"
        resourceType="Unspecified"
        type="dCForm.Core.DocRevHandler, dCForm, Version=1.0.0.0, Culture=neutral"/>
      <add
       name="dCFormRedirector_Classic"
       preCondition="classicMode,runtimeVersionv4.0,bitness64"
       path="*"
       verb="*"
       modules="IsapiModule"
       scriptProcessor="C:\Windows\Microsoft.NET\Framework64\v4.0.30319\aspnet_isapi.dll"
       resourceType="Unspecified"
       requireAccess="None"
        />
    </handlers>
  </system.webServer>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <httpHandlers>
      <add verb="*" path="*.*" type="dCForm.Core.DocRevHandler, dCForm, Version=1.0.0.0, Culture=neutral" validate="false"/>
    </httpHandlers>
  </system.web>
</configuration> 